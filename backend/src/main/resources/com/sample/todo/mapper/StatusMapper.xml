<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sample.todo.mapper.StatusMapper">

    <!-- ステータスマスタ一覧取得 -->
    <select id="statusAll" resultType="StatusEntity">
        SELECT
            id,
            status_name
        FROM
            status_master
        ORDER BY
            id ASC
    </select>
</mapper>