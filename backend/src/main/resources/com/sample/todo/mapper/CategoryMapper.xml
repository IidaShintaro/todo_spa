<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sample.todo.mapper.CategoryMapper">

    <!-- カテゴリーマスタ一覧取得 -->
    <select id="categoryAll" resultType="CategoryEntity">
        SELECT
            id,
            category_name
        FROM
            category_master
        ORDER BY
            id ASC
    </select>
</mapper>